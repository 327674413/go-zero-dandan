type GetTokenReq {
	Appid  *string `json:"appid"`
	Secret *string `json:"secret"`
}

type GetTokenResp {
	Token     string `json:"token"`
	ExpireSec int64  `json:"expireSec""`
}
@server(
	middleware: MetaMiddleware
)
service plat-api {
	@handler GetToken
	post /plat/getToken(GetTokenReq) returns (GetTokenResp)
}