type UploadResp {
	Url      string `json:"url"`
	FileName string `json:"fileName"`
}
type UploadImgReq {
	WatermarkFlag int64 `form:"watermarkFlag"`
}
@server(
	timeout: 30s
	middleware: LangMiddleware
	jwt: Auth
)
service asset-api {
	@handler UploadImg
	post /uploadImg(UploadImgReq) returns (UploadResp)
	@handler UploadFile
	post /uploadFile returns (UploadResp)
}