type UploadResp {
	Url string `json:"url"`
}
type UploadImgReq {
	WatermarkFlag int64 `fomr:"watermarkFlag"`
}
@server(
	middleware: LangMiddleware
)
service asset-api {
	@handler UploadImg
	post /uploadImg(UploadImgReq) returns (UploadResp)
	@handler UploadFile
	post /uploadFile returns (UploadResp)
}